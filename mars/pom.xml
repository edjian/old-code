         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <modules>
        <module>mars-biz</module>
        <module>mars-api</module>
    </modules>

    <parent>
        <groupId>com.colourfulchina</groupId>
        <artifactId>inf-root</artifactId>
        <version>1.0.0</version>
    </parent>

    <artifactId>mars</artifactId>
    <version>1.0.0</version>
    <name>mars</name>
    <packaging>pom</packaging>
    <description>客乐芙行星系统之火星</description>

    <properties>
        <mars.version>1.0.0</mars.version>
        <tianyan.version>1.0.0</tianyan.version>
        <nuwa.version>1.0.0</nuwa.version>
        <member.version>1.0.0</member.version>
        <pangu.version>1.0.0</pangu.version>
        <yangjian.version>1.0.0</yangjian.version>
        <god-door.version>1.0.0</god-door.version>
