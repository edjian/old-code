                <groupId>com.colourfulchina</groupId>
                <artifactId>inf-base</artifactId>
                <version>1.0.0</version>
            </dependency>
            <dependency>
                <groupId>com.colourfulchina</groupId>
                <artifactId>tianyan-common-security</artifactId>
