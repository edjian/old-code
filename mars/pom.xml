            <dependency>
                <groupId>com.colourfulchina</groupId>
                <artifactId>pangu-taishang-api</artifactId>
                <version>${pangu.version}</version>
            </dependency>
            <dependency>
                <groupId>com.colourfulchina</groupId>
                <artifactId>yangjian-api</artifactId>
                <version>${yangjian.version}</version>
            </dependency>
            <dependency>
                <groupId>com.colourfulchina</groupId>
                <artifactId>aggregate-pay-api</artifactId>
                <version>${aggregate.version}</version>
            </dependency>
            <dependency>
                <groupId>com.alibaba</groupId>
                <artifactId>druid</artifactId>
                <version>${druid.version}</version>
            </dependency>
            <dependency>
                <groupId>com.alibaba</groupId>
                <artifactId>druid-spring-boot-starter</artifactId>
                <version>1.1.21</version>
            </dependency>
