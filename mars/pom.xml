            </dependency>
            <dependency>
                <groupId>com.colourfulchina</groupId>
                <artifactId>nuwa-api</artifactId>
                <version>${nuwa.version}</version>
            </dependency>
            <dependency>
            	<groupId>com.fasterxml.jackson.dataformat</groupId>
